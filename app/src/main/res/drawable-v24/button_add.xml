<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="oval"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#F93535" />
            <gradient android:angle="-90" android:startColor="#F93535" android:endColor="#F93535"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval"  >

            <stroke android:width="1dip" android:color="#F93535" />
            <solid android:color="#F93535"/>
        </shape>
    </item>
    <item >
        <shape android:shape="oval"  >

            <stroke android:width="1dip" android:color="#F94C4C" />
            <gradient android:angle="-90" android:startColor="#EC4C4C" android:endColor="#D14B4B" />
        </shape>
    </item>
</selector>